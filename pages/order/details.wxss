page {
  width: 100%;
  height: 100%;
  position:relative;
  background-color: #FFF;
}

.order-address{
  margin-top: 10rpx;
}

.goods-list {
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
}

.order-details{
  margin: 30rpx 30rpx;
  width: 690rpx;
  border: solid 1rpx #eeeeee;

}

.title{
  height: 40rpx;
}

.order-time{
  margin-top: 20rpx;
}

.order-time view{
  width: 650rpx;
  height: 24rpx;
  margin: 20rpx 50rpx;
  font-size: 24rpx;
}

.order-time view text{
  color: #999999;
}

.details-button {
  position: fixed;
  width: 750rpx;
  height:92rpx;
  bottom: 0px;
  right: 0px;
  border-top: solid 2rpx #eeeeee;
}

.order-status-button{
  float: right;
  height: 90rpx;
  flex-direction:row;
  display: flex;
  margin-right: 30rpx;
}

.order-button{
  height: 34rpx;
  width: 100rpx;
  margin: 26rpx 16rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 18rpx;
  border-radius: 6px 6px 6px 6px;
}

.order-button-error{
  border: solid 2rpx #b5b5b5;
  color: #b5b5b5;
}

.order-button-success{
  border: solid 2rpx #f79307;
  color: #f79307;
}

