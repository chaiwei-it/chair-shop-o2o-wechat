page{
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.goods-list {
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
}

.order-details{
  margin: 30rpx 30rpx;
  width: 690rpx;
  border: solid 1rpx #eeeeee;

}

.title{
  height: 40rpx;
}

.order-time{
  margin-top: 20rpx;
}

.order-time view{
  width: 650rpx;
  height: 24rpx;
  margin: 20rpx 50rpx;
  font-size: 24rpx;
}

.order-time view text{
  color: #999999;
}

.order-rebate{
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
  border: solid 1rpx #eeeeee;
  padding: 20rpx 0;
}

.rebate{
  height: 40rpx;
  flex-direction:row;
  display: flex;
}

.rebate-name, .rebate-price{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
}

.rebate-name{
  width: 560rpx;
  text-align: right;
  color: #333333;
}

.rebate-price{
  margin-left: 30rpx;
  width: 100rpx;
  text-align: left;
  color: #ff0000
}

.rebate-total{
  margin-top: 10rpx;
  flex-direction:row;
  display: flex;
}

.rebate-total view{
  font-size: 20rpx;
}